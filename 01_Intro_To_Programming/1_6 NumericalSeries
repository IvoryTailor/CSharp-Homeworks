using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _1_6_Numerical_Series
{
    class Program
    {
        static void Main(string[] args)
        {
            for(int i=1;i<=102;i*=-1)
            {
                if (i <= 0)
                    i--;
                else
                    i++;
                System.Console.WriteLine(i);
            }
        }
    }
}
