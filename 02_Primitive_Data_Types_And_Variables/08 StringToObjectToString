using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace StringToObjectToString
{
    class Program
    {
        static void Main(string[] args)
        {
            string hello = "Hello";
            string world = "World";
            object fullString = hello + " " + world;
            string objectFullString = (string)fullString;
            Console.WriteLine(objectFullString);
        }
    }
}
