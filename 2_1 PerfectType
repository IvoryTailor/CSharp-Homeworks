using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace PerfectType
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine(SByte.MaxValue);
            Console.WriteLine(Byte.MaxValue);
            Console.WriteLine(short.MaxValue);
            Console.WriteLine(ushort.MaxValue);
            Console.WriteLine(int.MaxValue);
            Console.WriteLine(uint.MaxValue);
            Console.WriteLine(long.MaxValue);
            Console.WriteLine(ulong.MaxValue);

            Console.WriteLine();

            ushort unsignedShort = 52130;
            Console.WriteLine(unsignedShort);
            sbyte shortByte = -115;
            Console.WriteLine(shortByte);
            int integer1 = 4825932;
            Console.WriteLine(integer1);
            byte byte1 = 97;
            Console.WriteLine(byte1);
            int integer2 = -100000;
            Console.WriteLine(integer2);
            ushort unsignedShort2 = 1990;
            Console.WriteLine(unsignedShort2);
            long long1 = 123456789123456789;
            Console.WriteLine(long1);

        }
    }
}
